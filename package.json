{
  "name": "naver-finance-crawl-mcp",
  "version": "1.0.1",
  "description": "Naver Finance web crawler with MCP server support - crawl Korean stock market data",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "naver-finance-crawl-mcp": "dist/mcp-server.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "build": "tsc",
    "dev": "tsx watch src/mcp-server.ts",
    "dev:http": "tsx watch src/http-server.ts",
    "start": "node dist/mcp-server.js",
    "start:http": "node dist/http-server.js",
    "prepublishOnly": "pnpm run typecheck && pnpm run lint && pnpm run test && pnpm run build"
  },
  "keywords": [
    "crawler",
    "finance",
    "web-scraping",
    "naver",
    "naver-finance",
    "korean-stocks",
    "mcp",
    "mcp-server",
    "stock-market",
    "cheerio",
    "axios"
  ],
  "author": "greatsumini",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greatsumini/finance-crawl.git"
  },
  "homepage": "https://github.com/greatsumini/finance-crawl#readme",
  "bugs": {
    "url": "https://github.com/greatsumini/finance-crawl/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^14.0.2",
    "express": "^5.1.0",
    "iconv-lite": "^0.7.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
}
